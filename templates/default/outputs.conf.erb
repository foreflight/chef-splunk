[tcpout]
defaultGroup = splunk_indexers_<%= node['splunk']['receiver_port'] %>
disabled=false

[tcpout:splunk_indexers_<%= node['splunk']['receiver_port'] %>]
server=<%= @splunk_servers.map{ |s| "#{s['ipaddress']}:#{s['splunk']['receiver_port']}" }.join(', ') %>
<% @node['splunk']['outputs_conf'].each_pair do |name, value| -%>
<%= name %> = <%= value %>
<% end -%>
